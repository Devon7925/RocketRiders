execute as @a[team=!Lobby,team=!Spectator,team=!Developer] at @s if entity @s[z=170,dz=180] run advancement grant @s only achievements:rr_challenges/commited
execute as @a[team=!Lobby,team=!Spectator,team=!Developer] at @s if entity @s[z=-170,dz=-180] run advancement grant @s only achievements:rr_challenges/commited